{
  "name": "thor-discord-bot",
  "type": "module",
  "packageManager": "pnpm@10.2.1",
  "prettier": "@in5net/prettier-config",
  "main": "./src/main.ts",
  "scripts": {
    "sync": "svelte-kit sync",
    "dev": "tsx watch src/main.ts & vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "start": "tsx src/main.ts",
    "debug": "tsx --inspect-brk src/main.ts",
    "fmt": "prettier . --write --cache --ignore-unknown",
    "fmt:check": "prettier . --check --cache --ignore-unknown",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:fix-dry": "eslint --fix-dry-run",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "deploy": "tsx src/deploy.ts"
  },
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/rest": "2.4.2",
    "@discordjs/voice": "0.18.0",
    "@googleapis/customsearch": "3.2.0",
    "@in5net/std": "^0.16.1",
    "@napi-rs/canvas": "0.1.67",
    "@paralleldrive/cuid2": "^2.2.2",
    "better-sqlite3": "^11.8.1",
    "chalk-template": "^1.1.0",
    "cheerio": "1.0.0",
    "chess.js": "1.0.0",
    "decode-gif": "^1.0.1",
    "deepl-node": "^1.16.0",
    "discord-api-types": "0.37.119",
    "discord.js": "14.17.3",
    "discord.js-minesweeper": "^1.1.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.39.2",
    "emoji-mixer": "^1.1.12",
    "fluent-ffmpeg": "^2.1.3",
    "fuse.js": "^7.1.0",
    "genius-lyrics": "4.4.7",
    "gif-encoder": "^0.7.2",
    "gl": "^8.1.6",
    "gl-matrix": "^3.4.3",
    "glsl-noise": "^0.0.0",
    "glslify": "^7.1.1",
    "got": "14.4.5",
    "libsodium-wrappers": "^0.7.15",
    "magic-regexp": "^0.8.0",
    "ms": "^2.1.3",
    "musescore-metadata": "^3.0.0",
    "nanoid": "5.0.9",
    "nekos-best.js": "^6.3.0",
    "node-schedule": "^2.1.1",
    "owofire": "^1.0.4",
    "pino": "^9.6.0",
    "play-dl": "1.9.7",
    "pngjs": "^7.0.0",
    "prism-media": "^1.3.5",
    "replicate": "^1.0.1",
    "svelte-dnd-action": "^0.9.57",
    "syllable": "^5.0.1",
    "tiny-typed-emitter": "^2.1.0",
    "trkl": "^2.0.1",
    "tsx": "^4.19.2",
    "youtubei.js": "13.0.0",
    "zod": "^3.24.1",
    "zx": "^8.3.2"
  },
  "devDependencies": {
    "@in5net/eslint-config": "^0.15.0",
    "@in5net/prettier-config": "^1.4.2",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.17.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/better-sqlite3": "^7.6.12",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/gif-encoder": "^0.7.4",
    "@types/gl": "^6.0.5",
    "@types/ms": "^2.1.0",
    "@types/node": "22.13.1",
    "@types/node-schedule": "^2.1.7",
    "@types/pngjs": "^6.0.5",
    "drizzle-kit": "^0.30.4",
    "eslint": "^9.19.0",
    "eslint-plugin-svelte": "^2.46.1",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.4.2",
    "svelte": "^5.19.9",
    "svelte-check": "^4.1.4",
    "typescript": "5.7.3",
    "vite": "^6.1.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@discordjs/opus",
      "better-sqlite3",
      "esbuild",
      "gl"
    ]
  }
}
